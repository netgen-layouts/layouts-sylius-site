{
    "name": "netgen/layouts-sylius-site",
    "type": "project",
    "description": "Starting point for projects powered by Sylius eCommerce and Netgen Layouts.",
    "keywords": [
        "symfony",
        "sylius",
        "distribution",
        "ecommerce",
        "cart",
        "shopping",
        "netgen",
        "layouts"
    ],
    "homepage": "https://github.com/netgen-layouts/layouts-sylius-site",
    "license": "MIT",
    "authors": [
        {
            "name": "Paweł Jędrzejewski"
        },
        {
            "name": "Sylius project"
        },
        {
            "name": "Community contributions"
        },
        {
            "name": "Netgen",
            "homepage": "https://netgen.io"
        }
    ],
    "require": {
        "php": "^8.4",
        "sylius/mollie-plugin": "^3.2",
        "sylius/paypal-plugin": "^2.0",
        "sylius/sylius": "^2.2",
        "symfony/dotenv": "^7.4",
        "symfony/flex": "^2.10",
        "symfony/runtime": "^7.4",

        "netgen/layouts-standard": "~2.0.0",
        "netgen/layouts-sylius": "~2.0.0"
    },
    "require-dev": {
        "behat/behat": "^3.2",
        "behat/mink-selenium2-driver": "^1.7",
        "dbrekelmans/bdi": "^1.4",
        "dmore/behat-chrome-extension": "^1.4",
        "dmore/chrome-mink-driver": "^2.10",
        "friends-of-behat/mink": "^1.11",
        "friends-of-behat/mink-browserkit-driver": "^1.6",
        "friends-of-behat/mink-debug-extension": "^2.1",
        "friends-of-behat/mink-extension": "^2.7",
        "friends-of-behat/page-object-extension": "^0.3",
        "friends-of-behat/suite-settings-extension": "^1.1",
        "friends-of-behat/symfony-extension": "^2.6",
        "friends-of-behat/variadic-extension": "^1.6",
        "lchrusciel/api-test-case": "^5.3",
        "nyholm/psr7": "^1.8",
        "phpstan/extension-installer": "^1.4",
        "phpstan/phpstan": "^2.1",
        "phpstan/phpstan-doctrine": "^2.0",
        "phpstan/phpstan-webmozart-assert": "^2.0",
        "phpunit/phpunit": "^10.5",
        "robertfausk/behat-panther-extension": "^1.2",
        "sylius-labs/coding-standard": "^4.5",
        "sylius-labs/suite-tags-extension": "~0.2",
        "sylius/sylius-rector": "^3.7",
        "symfony/browser-kit": "^7.4",
        "symfony/contracts": "^3.6",
        "symfony/debug-bundle": "^7.4",
        "symfony/http-client": "^7.4",
        "symfony/intl": "^7.4",
        "symfony/web-profiler-bundle": "^7.4"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": false,
            "phpstan/extension-installer": false,
            "rector/extension-installer": true,
            "symfony/flex": true,
            "symfony/runtime": true,
            "symfony/thanks": false,
            "*": false
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        },
        "symfony": {
            "allow-contrib": true,
            "require": "^7.4",
            "endpoint": [
                "https://api.github.com/repos/Sylius/SyliusRecipes/contents/index.json?ref=flex/main",
                "https://api.github.com/repos/netgen-layouts/recipes/contents/index.json?ref=flex",
                "flex://defaults"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        },
        "classmap": [
            "src/Kernel.php"
        ]
    },
    "prefer-stable": true,
    "scripts": {
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "post-create-project-cmd": [
            "@php bin/console sylius:inform-about-gus --ansi",
            "@php bin/console sylius:show-plus-info --ansi"
        ],
        "rector-dry-run": "vendor/bin/rector process --dry-run",
        "rector-apply": "vendor/bin/rector process",
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
        }
    },
    "replace": {
        "symfony/polyfill-apcu": "*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-intl": "*",
        "symfony/polyfill-intl-grapheme": "*",
        "symfony/polyfill-intl-icu": "*",
        "symfony/polyfill-intl-idn": "*",
        "symfony/polyfill-intl-normalizer": "*",
        "symfony/polyfill-mbstring": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*",
        "symfony/polyfill-php83": "*"
    }
}
